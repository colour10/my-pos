{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "appstract/laravel-opcache": "^2.0",
        "aws/aws-sdk-php": "~3.0",
        "baijunyao/laravel-print": "^3.0",
        "barryvdh/laravel-cors": "^0.11.0",
        "caouecs/laravel-lang": "~3.0",
        "chenhua/laravel5-kindeditor": "^1.0",
        "fideloper/proxy": "^4.0",
        "gregwar/captcha": "1.*",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/mongodb": "^3.4",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.3",
        "laravel/scout": "^4.0",
        "laravel/tinker": "^1.0",
        "laravelchen/laravel-editormd": "^2.5",
        "maatwebsite/excel": "~2.1",
        "mews/captcha": "^2.2",
        "mews/purifier": "^2.1",
        "overtrue/laravel-wechat": "~4.0",
        "pda/pheanstalk": "~3.0",
        "phpunit/phpunit": "^7.4",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "1.3.*",
        "spatie/laravel-backup": "^5.9",
        "stevenyangecho/laravel-u-editor": "~1.4",
        "tamayo/laravel-scout-elastic": "^3.0",
        "webpatser/laravel-uuid": "^3.0",
        "zhuzhichao/bank-card-info": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
